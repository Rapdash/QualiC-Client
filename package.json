            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "firebase": "^7.4.0",
        "react": "^16.12.0",
        "react-bootstrap": "^1.0.0-beta.15",
        "react-dom": "^16.12.0",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0"
    },
    "devDependencies": {
        "husky": "^3.1.0",
        "lint-staged": "^9.4.3",
        "prettier": "^1.19.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*/.{js,jsx,md,json,css,html}": [
            "prettier --single-quote --jsx-single-quote --save",
